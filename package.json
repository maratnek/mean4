{
  "name": "mean4",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "buildwatch": "ng build -w",
    "build-i18n":"ng build --aot -op=deploy -prod --i18n-file=src/i18n/messages.xlf --i18n-format=xlf --locale=en && npm run build-i18n-other",
    "build-i18n-other":"for lang in ru; do ng build --aot -op=deploy/$lang -prod --bh /$lang/ --i18n-file=src/i18n/messages.$lang.xlf --i18n-format=xlf --locale=$lang; done",
    "build-i18n-en-watch":"ng build -w --aot -op=deploy -prod --i18n-file=src/i18n/messages.xlf --i18n-format=xlf --locale=en",
    "build-i18n-ru-watch":"ng build -w --aot -op=deploy/ru -prod --i18n-file=src/i18n/messages.ru.xlf --i18n-format=xlf --locale=ru",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "dist": "build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.4.6",
    "@angular/cdk": "^2.0.0-beta.12",
    "@angular/common": "^4.4.6",
    "@angular/compiler": "^4.4.6",
    "@angular/core": "^4.4.6",
    "@angular/forms": "^4.4.6",
    "@angular/http": "^4.4.6",
    "@angular/material": "^2.0.0-beta.12",
    "@angular/platform-browser": "^4.4.6",
    "@angular/platform-browser-dynamic": "^4.4.6",
    "@angular/router": "^4.4.6",
    "angular-font-awesome": "^2.3.7",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "core-js": "^2.5.1",
    "electron-compile": "^6.4.2",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.16.2",
    "font-awesome": "^4.7.0",
    "gm": "^1.23.0",
    "hammerjs": "^2.0.8",
    "keymaster": "^1.6.2",
    "lodash": "^4.17.4",
    "materialize-css": "^0.100.2",
    "mongodb": "^2.2.33",
    "request": "^2.83.0",
    "rxjs": "^5.5.0",
    "strip": "^3.0.0",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "1.4.9",
    "@angular/compiler-cli": "^4.4.6",
    "@angular/language-service": "^4.4.6",
    "@types/jasmine": "~2.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.0.46",
    "co": "^4.6.0",
    "codelyzer": "~3.2.1",
    "electron-builder": "^19.37.2",
    "electron-prebuilt-compile": "1.7.9",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.2.0",
    "ts-node": "~3.3.0",
    "tslint": "~5.7.0",
    "typescript": "~2.5.3"
  },
  "main": "app.js",
  "author": "zm <maratn566ek@gmail.com>",
  "description": "stock story",
  "build": {
    "appId": "stock",
    "productName": "stock",
    "asar": "false",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    },
    "linux": {
      "category": "X-Office",
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {},
      "electronWinstallerConfig": {
        "name": ""
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": ""
      }
    }
  }
}
